{
  "name": "Enable Right Click",
  "version": "1.2.1",
  "description": "Enables right clicking on pages that have it disabled.",
  "manifest_version": 3,
  "icons": { 
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  },
  "action": {},
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
