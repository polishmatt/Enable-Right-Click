{
  "name": "Enable Right Click",
  "version": "1.2",
  "description": "Enables right clicking on pages that have it disabled.",
  "manifest_version": 2,
  "icons": { 
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["onload.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "permissions": [
    "tabs", "<all_urls>"
  ]  
}
